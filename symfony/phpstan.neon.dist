parameters:
  # Use the highest ruleset for stricter checks. You can lower to 7/6 if needed.
  level: max

  # Analyse your application code (and optionally tests)
  paths:
    - src

  # Store cache in var/ to avoid clutter
  tmpDir: var/phpstan

  # Reduce noise from mixed PHPDoc and generics until fully annotated
  reportUnmatchedIgnoredErrors: false

  # Autoloading/Bootstrap (Symfony doesn't need a bootstrap by default)
  # bootstrapFiles:
  #   - vendor/autoload.php

includes: []
